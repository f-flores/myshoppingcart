{
  "name": "schedulingapp",
  "version": "1.0.0",
  "description": "Scheduling App.",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://bitbucket.org/edgarsantos123/schedulingapp.git"
  },
  "author": "Edgar Santos, Fabian Flores",
  "license": "ISC",
  "bugs": {
    "url": "https://bitbucket.org/edgarsantos123/schedulingapp.git/issues"
  },
  "homepage": "https://murmuring-basin-78523.herokuapp.com/",
  "dependencies": {
    "axios": "^0.18.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.2",
    "cloudinary": "^1.11.0",
    "concurrently": "^3.6.0",
    "dotenv": "^5.0.1",
    "express": "^4.17.3",
    "express-mysql-session": "^2.0.1",
    "express-session": "^1.15.6",
    "formidable": "^1.2.1",
    "if-env": "^1.0.4",
    "moment": "^2.22.1",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "mysql2": "^1.6.1",
    "nodemon": "^1.18.4",
    "npm": "^6.4.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "sequelize": "^4.37.7",
    "uuid": "^3.2.1"
  }
}
