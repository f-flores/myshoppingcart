{
  "name": "myshoppingcart",
  "version": "1.0.0",
  "description": "Accelerate your learning of new tech with a community that lets you get to the heart of burning tech questions.  Stop wasting time and get to the core of things in less than 15 minutes.",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/f-flores/myshoppingcart.git"
  },
  "author": "Fabian Flores",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/f-flores/myshoppingcart/issues"
  },
  "homepage": "https://github.com/f-flores/myshoppingcart#readme",
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.2",
    "cloudinary": "^1.11.0",
    "concurrently": "^3.6.0",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "express-handlebars": "^3.0.0",
    "express-session": "^1.15.6",
    "formidable": "^1.2.1",
    "handlebars": "^4.0.11",
    "if-env": "^1.0.4",
    "moment": "^2.22.1",
    "multer": "^1.3.0",
    "mysql2": "^1.5.3",
    "nodemon": "^1.18.3",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "sequelize": "^4.37.7",
    "uuid": "^3.2.1"
  }
}
